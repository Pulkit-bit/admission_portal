<%- include('../links.ejs') %>
<%- include('../header.ejs') %>
<%- include('../videspart.ejs') %>


<h1 class="bg-info text-center mt-5">Course View</h1>
<div class="container">
    <table class="table table-bordered text-center">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Course</th>
                <th>Status</th>
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
                <tr>
                    <td><%= c.id %></td>
                    <td><%= c.name %></td>
                    <td><%= c.email %></td>
                    <td><%= c.course %></td>
                    <td><%= c.status %></td>
                    <td><%= c.comment %></td>
                </tr>
        </tbody>
    </table>
    
    <a href="/courseDisplay" class="btn btn-info">Back</a>
</div>

<%- include('../footer.ejs') %>
